                                <a href="/post/<%= post._id %>">Ekleyen: Admin</a>
                            </div>
                            <h3 class="blog-title box-title"><a href="/post/<%= post._id %>"><%= post.title %></a></h3>
                            <a href="/post/<%= post._id %>" class="link-btn style2"><i class="fas fa-arrow-right"></i> Devamını Oku</a>
                        </div>
                    </div>
                    <% }) %>
